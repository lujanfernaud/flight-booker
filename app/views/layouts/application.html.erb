<!DOCTYPE html>
<html>
  <head>
    <title>Fly With YAWO</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

    <%= render "header" %>

    <div class="container">
      <div class="row">
        <div class="col">
          <%= render "flash_messages" %>
          <%= yield %>
        </div>
      </div>
    </div>

  </body>
</html>
